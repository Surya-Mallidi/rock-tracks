 {
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
],
"parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
},
"parser": "babel-eslint",
"env": {
    "es6": true,
    "browser": true,
    "node": true
},
"rules": {
    "react/react-in-jsx-scope": "off",
   "react/prop-types": ["off"]
}
}